<%= simple_form_for(@organization) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :mane %>
    <%= f.input :email %>
    <%= f.input :subdomain %>
    <%= f.input :max_projects %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
