<h1>Listing other_names</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @other_names.each do |other_name| %>
      <tr>
        <td><%= other_name.name %></td>
        <td><%= other_name.start_date %></td>
        <td><%= other_name.end_date %></td>
        <td><%= other_name.note %></td>
        <td><%= link_to 'Show', other_name %></td>
        <td><%= link_to 'Edit', edit_other_name_path(other_name) %></td>
        <td><%= link_to 'Destroy', other_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Other name', new_other_name_path %>
