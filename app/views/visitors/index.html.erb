    <div class="row">
      <div class="col-md-12">
        <h2 class="text-white"><%= t('.title_orgs') %></h2>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3">
