<%= simple_form_for(@objective) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.association :solutions, as: :check_boxes %>
    <%= f.input :prioritized %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
