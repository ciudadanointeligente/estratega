<% content_for :breadcrumb do %>
  <div class="breadcrumb_bg">
    <ol class="breadcrumb container">
      <li>
        <%= link_to "Projects", projects_path %>
      </li>
      <li class="active"><%= truncate(@project.title, length: 30) %></li>
    </ol>
  </div>
<% end %>

<h2><%= @project.title %></h2>
<p><%= @project.description %></p>
<hr>
<div role="tabpanel">
  <ul class="nav nav-tabs" role="tablist">
    <li role="presentation" class="active"><a href="#stages" aria-controls="stages" role="tab" data-toggle="tab">Stages</a></li>
    <li role="presentation"><a href="#dashboard" aria-controls="dashboard" role="tab" data-toggle="tab">Dashboard</a></li>
    <li role="presentation"><a href="#documents" aria-controls="documents" role="tab" data-toggle="tab">Documents</a></li>
  </ul>

  <!-- Tab panes -->
  <div class="tab-content">
    <div role="tabpanel" class="tab-pane active" id="stages">
      
    <div class="row">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title" title="Complete Stage"><a data-toggle="collapse" href="#stage1" aria-expanded="false" aria-controls="collapseExample"><i class="fa fa-check-circle"></i> Stage 1: Articulate problems and policy solutions</a></h4>   
      </div>      
      <div class="collapse steps" id="stage1">
        <table class="table table-bordered">
            <tbody>
              <!-- real problem -->
              <tr title="Complete Step">
                <td class="text-center td-w-10p">
                  <% if !@project.real_problem.blank? %>
                  <i class="fa fa-unlock"></i>
                  <% else %>
                  <i class="fa fa-lock"></i>
                  <% end %>
                </td>
                <td>
                  <% if !@project.real_problem.blank? %>
                  <%= link_to @project.real_problem.title, stage1_project_path  %>
                  <% else %>
                  <%= link_to 'Articulate the real-world problem children are facing that you want to address', stage1_project_path  %>
                  <% end %>
                </td>
              </tr>
              <!-- policy problems -->
              <tr title="Complete Step">
                <td class="text-center td-w-10p">
                  <% if !@project.real_problem.blank? %>
                    <% if !@project.real_problem.policy_problems.blank? %>
                      <i class="fa fa-unlock"></i>
                    <% else %>
                      <i class="fa fa-lock"></i>
                    <% end %>
                  <% else %>
                  <i class="fa fa-lock"></i>
                  <% end %>
                </td>
                <td>
                  <% if !@project.real_problem.blank? %>
                    <% if !@project.real_problem.policy_problems.blank? %>
                      <%= link_to "Identify policy dimensions of the 'real-world' problem ( #{@project.real_problem.policy_problems.size} )", stage1_project_path  %>
                    <% else %>
                      <%= link_to "Identify policy dimensions of the 'real-world' problem", stage1_project_path  %>
                    <% end %>
                  <% else %>
                      <%= link_to "Identify policy dimensions of the 'real-world' problem", stage1_project_path  %>
                  <% end %>
                </td>
              </tr>
              <!-- policy solutions -->
              <tr title="Complete Step">
                <td class="text-center td-w-10p">
                  <% if !@project.real_problem.blank? %>
                      <% if !@project.real_problem.policy_problems.first.solutions.blank? %>
                        <i class="fa fa-unlock"></i>
                    <% else %>
                      <i class="fa fa-lock"></i>
                    <% end %>
                  <% else %>
                      <i class="fa fa-lock"></i>
                  <% end %>
                </td>
                <td>
                  <% if !@project.real_problem.blank? %>
                    <% if !@project.real_problem.policy_problems.first.blank? %>
                      <% @project.real_problem.policy_problems.each do |pp| %>
                        <%= link_to "Policy solutions: Describe the main policy solutions that would constitute an appropriate response (#{pp.solutions.size})", stage1_project_path  %>
                      <% end %>
                    <% else %>
                      <%= link_to "Policy solutions: Describe the main policy solutions that would constitute an appropriate response", stage1_project_path %>
                    <% end %>
                  <% else %>
                  <%= link_to "Policy solutions: Describe the main policy solutions that would constitute an appropriate response", stage1_project_path %>
                  <% end %>
                </td>
              </tr>
            </tbody>
      </table>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title" title="You must complete the previous stage"><a data-toggle="collapse" href="#stage2" aria-expanded="false" aria-controls="collapseExample"><i class="fa fa-times-circle"></i> Stage 2: Articulate and prioritize objectives (Example)</a></h4>   
      </div>      
      <div class="collapse steps" id="stage2">
        <table class="table table-bordered">
            <tbody>
              <tr title="You must complete the previous step">
                <td class="text-center td-w-10p"><i class="fa fa-lock"></i></td>
                <td>Add and prioritize objectives</td>
              </tr>
            </tbody>
      </table>
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title" title="Complete Stage"><a data-toggle="collapse" href="#stage3" aria-expanded="false" aria-controls="collapseExample"><i class="fa fa-check-circle"></i> Stage 2: Articulate and prioritize objectives</a></h4>   
      </div>      
      <div class="collapse steps" id="stage3">
        <table class="table table-bordered">
            <tbody>
              <tr title="Complete Step">
                <td class="text-center td-w-10p"><i class="fa fa-unlock"></i></td>
                <td><a href="#">Add and prioritize objectives</a></td>
              </tr>
            </tbody>
      </table>
      </div>
    </div>
  </div>
</div>
<hr>

<h4 class="title_goals">Stage 3: Set Goals</h4>
<% @objectives.each do |objective| %>
  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-default">
        <div class="panel-heading">
          <table class="table_goals">
          <tr title="Stage Uninitiated or Incomplete">
            <td class="td-w-3p"><i class="fa fa-chevron-circle-right pull-left"></i></td>
            <td><a data-toggle="collapse" href="#goal1" aria-expanded="false" aria-controls="collapseExample"><%= objective.title %></a></td>
          </tr>
          </table> 
        </div>      
        <div class="collapse steps in" id="goal1">
          <table class="table table-bordered">
              <tbody>
                <tr title="Uninitiated Step">
                  <td class="text-center td-w-10p">
                    <i class="fa fa-unlock"></i>
                  </td>
                  <td><%= link_to "Understand the institutional and decision-making cycles/Policy process", stage3_project_objective_path(@project, objective) %></td>
                </tr>
                <tr title="You must complete the previous step">
                  <td class="text-center td-w-10p">
                    <% if @a_size %>
                    <i class="fa fa-check"></i>
                    <% else %>
                    <i class="fa fa-unlock"></i>
                    <% end %>
                  </td>
                  <td>
                    <%= link_to "List actors and power map", stage3_project_objective_path(@project, objective, anchor: "actors") %>
                  </td>
                </tr>
                <tr title="You must complete the previous step">
                  <td class="text-center td-w-10p">
                    <% if @barriers_size || @factors_size %>
                    <i class="fa fa-check"></i>
                    <% else %>
                    <i class="fa fa-unlock"></i>
                    <% end %>
                  </td>
                  <td>
                    <%= link_to "Identify enabling factors and barriers", stage3_project_objective_path(@project, objective, anchor: "factors-barriers") %>
                  </td>
                </tr>
                <tr title="You must complete the previous step">
                  <td class="text-center td-w-10p">
                    <% if @outcomes_size %>
                    <i class="fa fa-check"></i>
                    <% else %>
                    <i class="fa fa-unlock"></i>
                    <% end %>
                  </td>
                  <td>
                    <%= link_to "Define the interim outcomes and activities", stage4_project_objective_path(@project, objective) %>
                  </td>
                </tr>
                <tr title="You must complete the previous step">
                  <td class="text-center td-w-10p">
                    <i class="fa fa-lock"></i>
                  </td>
                  <td>
                    <a href="#">Define theory of change</a>
                    <!-- # <%= link_to "Define theory of change", stage3_project_objective_path(@project, objective) %> -->
                  </td>
                </tr>
                <tr title="You must complete the previous step">
                  <td class="text-center td-w-10p">
                    <i class="fa fa-lock"></i>
                  </td>
                  <td>
                    <a href="#">Develop menssages</a>
                    <!-- # <%= link_to "Develop menssages", stage3_project_objective_path(@project, objective) %> -->
                  </td>
                </tr>
              </tbody>
        </table>
        </div>
      </div>
    </div>
  </div>
<% end %>
